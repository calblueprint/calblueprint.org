- project = t("projects.past_projects.#{params[:slug]}")
.project-banner
  .banner-inner
    h1.h0.title = project[:title]
    .project__image style="background-image: url(#{project[:banner_image]})"
.spacer
.project-case-study
  = raw @markdown.render project[:full_description]
= link_to "View Project", "#{project[:link]}", class: "button", target: '_blank'
.spacer
